{
    "name": "chapter-three/teamwork-api",
    "description": "Provides a guzzle 6+ api client for the Teamwork ReST API",
    "type": "library",
    "license": "MIT",
    "keywords": ["Teamwork", "PHP", "GuzzleHTTP"],
    "authors": [
        {
            "name": "Jaesin Mulenex",
            "email": "Jaesin@users.noreply.github.com"
        },
        {
            "name": "mfr",
            "email": "mfr@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle-services": "dev-guzzle6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "ChapterThree\\TeamworkAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Tests\\": "./vendor/guzzlehttp/guzzle/tests/"
        }
    },
    "minimum-stability": "dev"
}
